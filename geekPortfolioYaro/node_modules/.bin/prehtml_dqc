#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir//user/bin/env" ]; then
  "$basedir//user/bin/env" node "$basedir/../prehtml_dqc/prepros" "$@"
  ret=$?
else 
  /user/bin/env node "$basedir/../prehtml_dqc/prepros" "$@"
  ret=$?
fi
exit $ret
